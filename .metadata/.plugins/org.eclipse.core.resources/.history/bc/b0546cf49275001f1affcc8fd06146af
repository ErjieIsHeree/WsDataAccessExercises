package consolaJava;

public class DVDs extends Material{

	String director;
	
	public DVDs(String titulo, int anioPublicacion, String director) {
		super(titulo, anioPublicacion);
		this.director = director;
	}
	public String getDirector() {
		return director;
	}
	public void setDirector(String director) {
		this.director = director;
	}
	@Override
	public void mostrarInfo() {
		// TODO Auto-generated method stub
		System.out.println("Título: " + super.titulo);
		System.out.println("Año publicación: " + super.anioPublicacion);
	}
	
}
